

{
  "headers": [
    {
      "source": "/(.*\\.(?:webp|png|jpg|jpeg|gif|ico|svg|woff|woff2))$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/portfolio", "destination": "/portfolio.html" }
  ]
}